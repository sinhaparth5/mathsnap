{
  "name": "mathsnap",
  "version": "1.0.0",
  "description": "The goal is to provide a reusable component that abstracts away KaTeX configuration and makes it easy to use",
  "keywords": [
    "katex",
    "latex",
    "maths",
    "equations",
    "react",
    "svelte",
    "vue"
  ],
  "homepage": "https://github.com/sinhaparth5/mathsnap#readme",
  "bugs": {
    "url": "https://github.com/sinhaparth5/mathsnap/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinhaparth5/mathsnap.git"
  },
  "license": "MIT",
  "author": "Parth Sinha",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.mjs",
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "src/svelte",
    "src/vue"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs",
      "types": "./dist/react/index.d.ts"
    },
    "./svelte": "./src/svelte/MathEquation.svelte",
    "./vue": "./src/vue/MathEquation.vue"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "katex": "^0.16.21"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/katex": "^0.16.7",
    "@types/react": "^19.0.12",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.25.3",
    "ts-jest": "^29.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-dts": "^4.5.3",
    "vue": "*"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "vue": "^3.5.13"
  }
}
